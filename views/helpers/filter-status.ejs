<!-- Reuse status on more page -->
<% 
  filterStatusHelper = (statusFilter, keyword)=> {
    let xthmlResult = '';
    statusFilter.forEach((item) => {
      let link = systemConfig.prefixAdmin + '/item/' + item.valueStatus;
      if(keyword !== "" && keyword !== undefined) link += '?keyword=' + keyword;
      xthmlResult += `<a class="btn m-b-sm btn-${item.class} btn-sm" href="${link}">${item.name.toUpperCase() + " (" + item.count +")"}</a> `;
    })
    return xthmlResult;
  }
%>  